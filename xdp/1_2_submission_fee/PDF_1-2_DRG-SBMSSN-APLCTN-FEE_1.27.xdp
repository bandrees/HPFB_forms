/**
 * @private
 */
function _commentArg(expectedNumberOfNonCommentArgs, args)
{
    if (_argumentsIncludeComments(expectedNumberOfNonCommentArgs, args))
    {
        return args[0];
    }

    return null;
}

/**
 * @private
 */
function _displayStringForValue(aVar)
{
