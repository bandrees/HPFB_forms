                     <event activity="click" name="event__click">
                        <script contentType="application/x-javascript">var cLog = script.core.util.Log;

var englishPresence = this.resolveNode("sfInstructions.sfInstructionsContent.lblInstructions_en").presence;

var frenchPresence = this.resolveNode("sfInstructions.sfInstructionsContent.lblInstructions_fr").presence;

//cLog.trace("start clicking in English +++++++");
if (frenchPresence == "visible") {
	cLog.trace("start clicking in English +++++++" + frenchPresence);
	this.resolveNode("sfInstructions.sfInstructionsContent.lblInstructions_fr").presence = "hidden";
	this.resolveNode("sfInstructions.sfInstructionsContent.lblInstructions_en").presence = "visible";
}

this.resolveNode("DrugAppl.sfPage1.sfInstructions.sfInstructionBtns.txtGuideDocFrench").presence = "hidden";
this.resolveNode("DrugAppl.sfPage1.sfInstructions.sfInstructionBtns.txtGuideDocEnglish").presence = "visible";

this.resolveNode("DrugAppl.sfPage1.sfCertification.txtUndersigned_fr").presence = "hidden";
this.resolveNode("DrugAppl.sfPage1.sfCertification.txtUndersigned_en").presence = "visible";

var count = this.resolveNode('sfListStandardsTable._sfList').count;

for (var k = 0; k &lt; count; k++) {
	
	var temp = this.resolveNode("sfListStandardsTable.sfList[" + k + "].txtTitle").rawValue;

	//update StTitle
	if (temp != null) {

		var ddlList = null;
		ddlList = script.DrugAppl.resources.MedicalDeviceListAll.content.deviceAllList;

			
		var index = 5;
			for (var i = 0; i &lt; ddlList.length; i++) {
				if (ddlList[i][index] == temp) {
					this.resolveNode("sfListStandardsTable.sfList[" + k + "].txtTitle").rawValue=ddlList[i][index - 1];// .addItem(ddlList[i][index - 1], ddlList[i][index - 1]);
					break;
				}
			}
}	
