function _validateMonetaryValue(field) {
	// var regExp = ^\$?[0-9]+(,[0-9]{3})*(\.[0-9]{2})?$;
	var regExp = /^\$?(?:\d+|\d{1,3}(?:,\d{3})*)(?:\.\d{1,2}){0,1}$/;

	if (regExp.test(field.rawValue))
