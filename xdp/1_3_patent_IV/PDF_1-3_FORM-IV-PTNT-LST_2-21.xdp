		if(sfInstanceManager.count&lt;sfInstanceManager.max || sfInstanceManager.max&lt;0){
			var temp=cInstanceManagerUtil.addInstance(sfInstanceManager);
			cLog.trace(temp);
		}
	}	
	//TO DO add a log message?
}//end fn






