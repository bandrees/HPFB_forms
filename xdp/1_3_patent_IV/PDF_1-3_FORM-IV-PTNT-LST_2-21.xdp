function _updateImporterSameAsState(){

	var regMailCountry_node=REGMAIL_COUNTRY_DDL_FN.getNodeInstance();
	var importerSameAs_node=IMPORTER_SAMEAS_CHK_FN.getNodeInstance();
	
	//if node does not exist do not proceed
