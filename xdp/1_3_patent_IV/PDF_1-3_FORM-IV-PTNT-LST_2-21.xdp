			 if(instances==0){
			 
			 	cAppendix4Handler.removeAppendix4(appendixID);
			 }
			 
			//to do error handling
		}
		
	
	if(cSharedAppendix4Model.entryExists(ingredientName)&gt;-1){
		cSharedAppendix4Model.incrementWithEntry(ingredientName);
	}else{
