}

function isLeapYear(year) {
	return ((year % 4 == 0) &amp;&amp; (year % 100 != 0 || year % 400 == 0));
}

function readDaysInMonth(month, year) {
	if (month == 1 &amp;&amp; isLeapYear(year) == true)
		return 29;
	else
		return [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31][month];
}

