{
	switch(anEvent.getActionCommand())
	{
		
		case SPONSOR_COUNTRY_CHANGED_ACTION:
				_sponsorInfoCountryChanged(SPONSOR_PROVINCE_FN,SPONSOR_POSTAL_CODE_FN);					
			break;
			
				case CLINICAL_COUNTRY_CHANGED_ACTION:
				_clinicalCountryChanged(CLINICAL_PROVINCE_FN,CLINICAL_POSTAL_CODE_FN);					
			break;
			
				case CTSOLD_COUNTRY_CHANGED_ACTION:
				_ctsoldCountryChanged(CTSOLD_PROVINCE_FN,CTSOLD_POSTAL_CODE_FN);					
			break;
			
				case CTSNEW_COUNTRY_CHANGED_ACTION:
				_ctsnewCountryChanged(CTSNEW_PROVINCE_FN,CTSNEW_POSTAL_CODE_FN);					
			break;
				case QUI_COUNTRY_CHANGED_ACTION:
				_ctsoldCountryChanged(QUI_PROVINCE_FN,QUI_POSTAL_CODE_FN);					
			break;
			
				case REBA_COUNTRY_CHANGED_ACTION:
				_ctsnewCountryChanged(REBA_PROVINCE_FN,REBA_POSTAL_CODE_FN);					
			break;
			 	 
						
		default:
			throw cException.createInstance(SCRIPT_OBJECT_NAME
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}//switch
}//end fn


/*
* updates the UI based on the submission type that has been selected
*
*
*
*
*/

/*
*	relies on core delegate for updating address and state based on County
*	@param province_formNode- the province droplist to update
*	@param postal_formNode- the postal code textbox to update
*	@returns null
*
*
*
**/
function _applicantCountryChanged(province_formNode,postal_formNode){
	// Update correct labels to province/state and postal/state code and
	// clear the value entered in the postal/state code field
	province_formNode.updateUI();
	postal_formNode.updateUI();
	postal_formNode.getNodeInstance().rawValue = null;	
	//app.alert("test");
}//end fun



function _sponsorInfoCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
	
}//end fn

function _clinicalCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
		
}//end fn

function _ctsoldCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
	
}//end fn

function _ctsnewCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
	
}//end fn

function _quiCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
		
}//end fn

function _rebaCountryChanged(province_formNode,postal_formNode){

	//update the province, postal code labels
	_applicantCountryChanged(province_formNode,postal_formNode);
	
}//end fn

</script>
