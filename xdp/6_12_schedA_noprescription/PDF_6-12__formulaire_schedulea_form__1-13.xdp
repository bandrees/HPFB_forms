            <script contentType="application/x-javascript" name="nodeDefinition">

function nodeDefinition(outputField) {

    var nodeDefParentKey = ""; //parentName;
    var presentNodeDefParentKey = "";

    var allNode = "";

    var node4Resource = "";

    for (var i = 0; i &lt; xfa.host.numPages; i++) {
        var oFields = xfa.layout.pageContent(i);
        var hFields = xfa.nodes;
        var nodesLength = oFields.length;
        // FOR DEBUG: This correctly shows we accessed 9 items on this page (hidden items included) 
        // xfa.host.messageBox("hFields: " + hFields + " oFields " + nodesLength ,"li st of nodes"); 
        //going through all fields on the page.  nodeslength evaluates to 9 (10 items) here on my form. 
        var cSubform;
        for (var j = 0; j &lt; nodesLength; j++) {
            var oItem = oFields.item(j);
            oChildNode = oItem;

            if (oChildNode.className === "field" || oChildNode.className === "subform" || oChildNode.className === "draw" || oChildNode.className === "exclGroup" || oChildNode.className === "pageSet") {

                if (oChildNode.name === "script") {
                    continue;
                }
                var nodePropertyJS;
                var nodeDefAlias = oChildNode.name;
                var nodeDefSom = oChildNode.name;
                // var nodeDefParentKey="";//parentName;
               // var nodeDefLabelKey = "label" + oChildNode.name;
                var nodeDefLabelKey = oChildNode.name;
                var nodeDefDelegate = "";
                nodeDefParentKey = presentNodeDefParentKey;
                // if(oChildNode.className==="subform") {
                // nodeDefParentKey=oChildNode.name;
                //}
                if (testForRoot(oChildNode)) {
                    nodeDefAlias = "root";
                    nodeDefSom = "xfa.form." + oChildNode.name;
                    nodeDefParentKey = null;
                    nodeDefLabelKey = null;
                    nodeDefDelegate = null;
                    nodeDefFldNumber = 0;
                }

                //create the node alias definition
                nodePropertyJS = nodeDefAlias + ": {\n" + "\tsomExpression: \"" + nodeDefSom + "\",\n" + "\tparentNodeKey: \"" + nodeDefParentKey + "\"";

                //create the FieldIndex Key   
                if (oChildNode.className === "subform") {
                    presentNodeDefParentKey = oChildNode.name;
                }




                //create the label key
                if (oChildNode.className !== "subform") {
                    nodePropertyJS += ",\n" + "\tlabelKey: \"" + nodeDefLabelKey + "\"";
                    //  node4Resource + = "test";// ",\n" + nodeDefAlias +"\"";
                    var temp = oItem.caption.value.text.value
                    if (oChildNode.name.substring(0, 3) == "lbl") {
                        temp = oChildNode.rawValue;
                    }


                    node4Resource = node4Resource + "\n" + nodeDefSom + ":\t\"" + temp + "\",";

                    //node4Resource =node4Resource + ":\t\""+"\""+",\n"+ nodeDefSom;
                    //  app.alert("subform: " +oItem.caption.value.text.value);
                }

                nodePropertyJS += "},\n\n"

                allNode = allNode + nodePropertyJS;
                //app.alert(nodePropertyJS);
            }

        }



    }

    //node4Resource =node4Resource +  "test";
    outputField.rawValue = allNode + node4Resource;

    //app.alert(allNode + node4Resource);
}

/**
 * testForRoot- determines is a given node is the root node
 *@param oNode- the node to test
 *@returns- boolean, true if a root node
 **/
function testForRoot(oNode) {

    if (oNode.parent.name === "template" || oNode.parent.name === "form") {
        return true;
    }
    return false;
}
</script>
