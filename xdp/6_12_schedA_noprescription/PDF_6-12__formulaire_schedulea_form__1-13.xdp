                  <variables>
                     <script contentType="application/x-javascript" name="AttestationModule">
function initDsig() {
  console.println("AttestationModule.initDsig::script ..."); 
  AttestationModule.clearFields();
  sfSignatureDate.sfSignatureBlock.digitalSignatureField.presence = "visible";
  sfSignatureDate.sfSignatureBlock.wetSignatureField.presence = "hidden";
}

function initWetSig() {
  console.println("AttestationModule.initWetSig::script ...");
  AttestationModule.clearFields();
  this.resolveNode("sfSignatureDate.sfSignatureBlock.digitalSignatureField").presence = "hidden";
  this.resolveNode("sfSignatureDate.sfSignatureBlock.wetSignatureField").presence = "visible";
}

function isSigned() {
	return this.resolveNode("isSigned").rawValue === 1;
}


function makeSignable() {
    this.makeVisible();
    this.resolveNode("sfSignatureDate.sfSignatureBlock.wetSignatureField").access = "open";
    this.resolveNode("sfSignatureDate.sfSignatureBlock.digitalSignatureField").access = "open";
    this.resolveNode("sfSignatureRBG.signatureRBG.rbWetSig").access = "open";
    this.resolveNode("sfSignatureRBG.signatureRBG.rbDSig").access = "open";
    this.resolveNode("sfSignatureDate.dateSignedField").access = "readOnly";     
}

function hideAttestationBlock() {
	this.makeHidden();
	this.clearFields();
	this.resolveNode("sfSignatureDate.sfSignatureBlock.wetSignatureField").access = "readOnly";
	this.resolveNode("sfSignatureDate.sfSignatureBlock.digitalSignatureField").access = "readOnly";
	this.resolveNode("sfSignatureRBG.signatureRBG.rbWetSig").access = "readOnly";
	this.resolveNode("sfSignatureRBG.signatureRBG.rbDSig").access = "readOnly";
}

function makeVisible() {
  this.resolveNode("sfAttestation").presence = "visible";
 }

function makeHidden() {
  console.println("AttestationModule.makeHidden::script ...");
  this.resolveNode("sfAttestation").presence = "invisible";
}

function preSign() {
  console.println("AttestationModule.preSign::script ... perform pre-signing activities");
  this.setAttestationDate();
  this.resolveNode("isSigned").rawValue = 1;
}

function signWithWetSignature() {
    console.println("AttestationModule.signWithWetSignature::script ... mimic signing with digital signature");
	this.setAttestationDate();
	// Not sure about this here --- this.resolveNode("sfAttestation").access = "readOnly";
	this.resolveNode("sfAttestation").access = "readOnly";
	this.resolveNode("isSigned").rawValue = 1;
  	app.execMenuItem("SaveAs");}

function setAttestationDate() {
  console.println("AttestationModule.setAttestationDate::script ... setting date field to system date");
  this.resolveNode("sfSignatureDate.dateSignedField").rawValue = util.printd("yyyy-mm-dd", new Date());
}

function clearFields() {
  console.println("AttestationModule.clearFields::script ...");
  this.clearDsig();
  this.clearWetSig();
  this.clearAttestationDate();
  this.resolveNode("sfAttestation").access = "open";
  this.resolveNode("isSigned").rawValue = 0;
}

function clearDsig() {
  console.println("AttestationModule.clearDsig::script ... clear");
  this.resolveNode("sfSignatureDate.sfSignatureBlock.digitalSignatureField").rawValue = "";
}

function clearWetSig() {
  console.println("AttestationModule.clearWetsig::script ... clear");
  this.resolveNode("sfSignatureDate.sfSignatureBlock.wetSignatureField").rawValue = "";
}

function clearAttestationDate() {
  console.println("AttestationModule.clearAttestationDate::script ... clear date");
  this.resolveNode("sfSignatureDate.dateSignedField").rawValue = "";
}
</script>
